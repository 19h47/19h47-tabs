{
	"presets":
	[
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": ["last 2 versions"]
				},
				"debug": true
			}
		]
	],
	"plugins":
	[
		[
			"@babel/plugin-syntax-dynamic-import"
		],
		[
			"@babel/plugin-transform-runtime",
			{
				"corejs": false,
				"helpers": true,
				"regenerator": true,
				"useESModules": false
			}
		]
	]
}
